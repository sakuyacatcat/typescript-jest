{
  "name": "typescript-jest",
  "version": "1.0.0",
  "description": "TypeScript + Jest skeleton",
  "author": "sakuyacatcat",
  "keywords": [
    "typescript",
    "jest"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/sakuyacatcat/typescript-jest.git"
  },
  "scripts": {
    "fmt": "prettier --write *.ts lib/*.ts",
